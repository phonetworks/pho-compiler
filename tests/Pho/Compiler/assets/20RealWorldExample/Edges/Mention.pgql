# pho-graphql-v1

type Mention implements MentionEdge 
@nodes(tail:"Status", head:"User")
@properties(binding: false, persistent: true, consumer: true)
@labels(headSingular:"mention", headPlural: "mentions", tailSingular: "notification", tailPlural: "notifications")
{
    id: ID,
    status: String @constraints(maxLength:140)
}